{
  "name": "@template/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3030 --config vite.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "vite build --config vite.config.ts",
    "preview": "vite preview --config vite.config.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:integration:ui": "vitest --ui --config vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:coverage:integration": "vitest run --coverage --config vitest.integration.config.ts",
    "test:all": "bun run test && bun run test:integration",
    "wrangler:dev": "wrangler dev --port 3030",
    "wrangler:deploy": "wrangler deploy",
    "clean": "rm -rf .turbo node_modules dist coverage",
    "fonts:subset": "node scripts/subset-fonts.js",
    "fonts:optimize": "node scripts/optimize-fonts.js",
    "fonts:analyze": "node scripts/font-optimization-analysis.js",
    "fonts:check": "ls -lh public/fonts/",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "bundle:visualize": "vite build && open dist/bundle-analysis.html",
    "bundle:size": "bun run build && du -sh dist/**/*.js | sort -hr",
    "performance:summary": "node scripts/mobile-performance-summary.js"
  },
  "dependencies": {
    "@clerk/backend": "^2.0.0",
    "@clerk/tanstack-react-start": "^0.18.0",
    "@convex-dev/react-query": "0.0.0-alpha.11",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-three/drei": "^10.7.3",
    "@react-three/fiber": "^9.3.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@tanstack/react-router": "^1.121.134",
    "@tanstack/react-router-devtools": "^1.121.134",
    "@tanstack/react-router-with-query": "^1.121.134",
    "@tanstack/react-start": "^1.120.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@template/convex": "workspace:^",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-react": "^4.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.24.8",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.15.0",
    "geist": "^1.4.2",
    "ky": "^1.7.4",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.249.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redaxios": "^0.5.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.6.0",
    "three": "^0.179.1",
    "tiny-invariant": "^1.3.3",
    "tw-animate-css": "^1.3.2",
    "vaul": "^1.1.2",
    "web-vitals": "^5.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@tailwindcss/vite": "^4.1.7",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "msw": "^2.7.0",
    "tailwindcss": "^4.1",
    "vite": "6.0.5",
    "wrangler": "^4.23.0"
  }
}
